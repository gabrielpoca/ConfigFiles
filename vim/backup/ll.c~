#ifndef _STDIO
	#define _STDIO
	#include <stdio.h>
#endif
#ifndef _STRING
	#define _STRING
	#include <string.h>
#endif
#ifndef _STDLIB
	#define _STDLIB
	#include <stdlib.h>
#endif

#include "ll.h"

LLIGADA* ll_insert(LLIGADA *list, void *new_info) {
	LLIGADA *new_pointer = (LLIGADA *) malloc(sizeof(LLIGADA));
	new_pointer->next = list;
	new_pointer->info = new_info;
	return new_pointer;
}
