#ifndef _CLIENTES_H
#define _CLIENTES_H

#include "servicos.h"

typedef struct cliente
{ char *nome;
  char *morada;
  long int contribuinte;
  Servico *servicos;
  struct cliente *next;
} CLIENTE;


CLIENTE *clientes_new();
void clientes_insert(CLIENTE *c);
void clientes_remove(long int cliente_id);
void clientes_dump_list();
void clientes_init();
void clientes_list_all();

#endif
