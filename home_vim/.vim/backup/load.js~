$(document).ready(function() {
	var tableContents = 'a#table-contents';
	//$(tableContents).find('ul').css('visibility', 'hidden');
	$(tableContents).find('ul').hide();
	$(tableContents).hover(function() {
		$(this).find('ul').show();
	});
	$(tableContents).mouseleave(function() {
		$(this).find('ul').hide();
	});
});
