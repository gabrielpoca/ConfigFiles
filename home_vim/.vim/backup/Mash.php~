<?php

   class Mash extends AppModel {
	  var $useTable = 'mash';
	  var $belongsTo = array(
		 'Hot' => array(
			'className'  => 'Photo',
			'foreignKey' => 'hot_id'
		 ),
		 'NotHot' => array(
			'className'  => 'Photo',
			'foreignKey' => 'nothot_id'
		 ),
		 'Fbuser' => array(
			'className'	=> 'Fbuser',
			'foreignKey'	=> 'fbuser_id'
		 )
	  );
   }

?>
